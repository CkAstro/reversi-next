{
   "name": "reversi-next",
   "version": "0.1.1",
   "private": true,
   "scripts": {
      "dev": "next dev --turbopack",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "test": "test/run-jest.sh",
      "test:commit": "jest --clearCache && jest --watchAll=false",
      "docker": "npm run docker:dev",
      "docker:dev": "docker compose -f docker/docker-compose.dev.yml up",
      "spellcheck": "cspell --no-progress --dot \"**/*.{js,ts,jsx,tsx,json,html,xml,svg,css,scss,sass,md}\"",
      "prepare": "husky"
   },
   "dependencies": {
      "clsx": "^2.1.1",
      "next": "15.2.1",
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
   },
   "devDependencies": {
      "@eslint/eslintrc": "^3",
      "@tailwindcss/postcss": "^4",
      "@testing-library/dom": "^10.4.0",
      "@testing-library/jest-dom": "^6.6.3",
      "@testing-library/react": "^16.2.0",
      "@testing-library/user-event": "^14.6.1",
      "@types/jest": "^29.5.14",
      "@types/node": "^20",
      "@types/react": "^19",
      "@types/react-dom": "^19",
      "@typescript-eslint/parser": "^8.26.1",
      "cspell": "^8.17.5",
      "eslint": "^9",
      "eslint-config-next": "15.2.1",
      "husky": "^9.1.7",
      "jest": "^29.7.0",
      "jest-environment-jsdom": "^29.7.0",
      "tailwindcss": "^4",
      "ts-node": "^10.9.2",
      "typescript": "^5"
   }
}
